#include <stdio.h>
int main()
{
    int a[100000];
    int b[100000];
    int c[100000],i,N;
    scanf("%d\n",&N);
    for(i=0;i<N;i++)
    {
        scanf("%ld ",&a[i]);
    }
    for(i=0;i<N;i++)
    {
        scanf("%ld ",&b[i]);
    }
    for(i=0;i<N;i++)
    {
        
        c[i]=a[i]+b[i];
    }
    for(i=0;i<N;i++)
    {
    printf("%ld ",c[i]);
}
    return 0;
}
